@model List<Student>
@{
    ViewData["Title"] = "Students Page";
}

<h1>Students in Database</h1>

<div class="row">
    @foreach (var item in @Model)
    {
    <div class="card col-3">
        <div class="card-body">
            <h5 class="card-title">@item.Name</h5>
            <a asp-controller="Students" asp-action="Detail" asp-route-id="@item.Id" class="btn btn-primary">Detail</a>
        </div>
    </div>
    }
</div>